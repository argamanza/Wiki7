<div class="page-actions-more page-actions__item wiki7-dropdown">
	<details class="wiki7-dropdown-details">
		<summary
			class="wiki7-dropdown-summary"
			title="{{msg-wiki7-actions-more-toggle}}"
			aria-details="page-actions-more__card">
			<span class="wiki7-ui-icon mw-ui-icon-wikimedia-ellipsis"></span>
			<span>{{msg-wiki7-actions-more-toggle}}</span>
		</summary>
	</details>
	<aside id="page-actions-more__card" class="wiki7-menu__card">
		<div class="wiki7-menu__card-backdrop"></div>
		<div class="wiki7-menu__card-content">
			{{#is-visible}}
					{{#data-portlets.data-actions}}{{>Menu}}{{/data-portlets.data-actions}}
			{{/is-visible}}
			{{#data-article-tools}}{{>Menu}}{{/data-article-tools}}
		</div>
	</aside>
</div>